{
    "$help": "https://aka.ms/terminal-documentation",
    "$schema": "https://aka.ms/terminal-profiles-schema",
    "actions": 
    [
        {
            "command": "find",
            "keys": "ctrl+shift+f"
        },
        {
            "command": 
            {
                "action": "splitPane",
                "split": "auto",
                "splitMode": "duplicate"
            },
            "keys": "alt+shift+d"
        }
    ],
    "copyFormatting": "none",
    "copyOnSelect": true,
    "defaultProfile": "{3a949d14-75d8-4637-86cf-ad5c78cd2cb3}",
    "disableAnimations": true,
    "firstWindowPreference": "defaultProfile",
    "largePasteWarning": false,
    "multiLinePasteWarning": false,
    "newTabMenu": 
    [
        {
            "type": "remainingProfiles"
        }
    ],
    "profiles": 
    {
        "defaults": 
        {
            "closeOnExit": "always",
            "colorScheme": "One Half Dark",
            "font": 
            {
                "face": "JetBrainsMono Nerd Font",
                "size": 10.0
            },
            "padding": "0"
        },
        "list": 
        [
            {
                "commandline": "%SystemRoot%\\System32\\WindowsPowerShell\\v1.0\\powershell.exe",
                "guid": "{61c54bbd-c2c6-5271-96e7-009a87ff44bf}",
                "hidden": false,
                "name": "Windows PowerShell"
            },
            {
                "commandline": "%SystemRoot%\\System32\\cmd.exe",
                "guid": "{0caa0dad-35be-5f56-a8ff-afceeeaa6101}",
                "hidden": false,
                "name": "Command Prompt"
            },
            {
                "commandline": "%USERPROFILE%/scoop/shims/nu.exe",
                "cursorShape": "filledBox",
                "guid": "{3a949d14-75d8-4637-86cf-ad5c78cd2cb3}",
                "hidden": false,
                "name": "Nushell",
                "startingDirectory": "%USERPROFILE%"
            },
            {
                "commandline": "%USERPROFILE%/scoop/apps/git/current/usr/bin/bash.exe -i -l",
                "cursorShape": "filledBox",
                "guid": "{00000000-0000-0000-ba54-000000000002}",
                "hidden": false,
                "icon": "%USERPROFILE%/scoop/apps/git/current/mingw64/share/git/git-for-windows.ico",
                "name": "Git Bash",
                "startingDirectory": "%USERPROFILE%"
            },
            {
                "commandline": "%ProgramFiles%/Git/usr/bin/bash.exe -i -l",
                "cursorShape": "filledBox",
                "guid": "{00000000-0000-0000-ba54-000000000003}",
                "hidden": true,
                "icon": "%ProgramFiles%/Git/mingw64/share/git/git-for-windows.ico",
                "name": "Git Bash",
                "startingDirectory": "%USERPROFILE%"
            },
            {
                "guid": "{b453ae62-4e3d-5e58-b989-0a998ec441b8}",
                "hidden": true,
                "name": "Azure Cloud Shell",
                "source": "Windows.Terminal.Azure"
            },
            {
                "closeOnExit": "always",
                "cursorShape": "filledBox",
                "guid": "{2c4de342-38b7-51cf-b940-2309a097f518}",
                "hidden": false,
                "name": "Ubuntu",
                "source": "Windows.Terminal.Wsl",
                "startingDirectory": "~"
            }
        ]
    },
    "schemes": [],
    "tabSwitcherMode": "disabled",
    "tabWidthMode": "equal",
    "theme": "dark",
    "themes": [],
    "trimBlockSelection": true,
    "windowingBehavior": "useExisting",
    "wordDelimiters": " \"'=()[]{}<>,"
}
