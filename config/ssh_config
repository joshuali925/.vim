Host *
    ServerAliveInterval 30
    ServerAliveCountMax 1200
    Compression yes
    ControlMaster auto
    ControlPath ~/.ssh/master_%r_%C  # TOKEN section under man ssh_config

# Host *
#     ControlPersist 10h
#     ForwardAgent yes

Host *.trycloudflare.com
    HostName %h
    Port 22
    ProxyCommand cloudflared access ssh --hostname %h
