Host *
    ServerAliveInterval 47
    ServerAliveCountMax 5
    Compression yes
    ControlMaster auto
    # see TOKENS section under man ssh_config for formatting
    ControlPath ~/.ssh/_%r_%C
    # ControlPersist 10h
    # ForwardAgent yes
    # ForwardX11 yes
    # DynamicForward 1080

# temporary tunnel: cloudflared tunnel --url ssh://localhost:8022
# or log in: cloudflared tunnel login && cloudflared tunnel run --token xxx
Host *.trycloudflare.com
    HostName %h
    Port 22
    ProxyCommand cloudflared access ssh --hostname %h
