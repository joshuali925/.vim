{
  "include": {
    "prefix": "include",
    "body": ["Include ~/.ssh/${1:my_config}"]
  },
  "ssh config template": {
    "prefix": "template",
    "body": [
      "Host ${1:alias}",
      "\tHostName ${2:ip}",
      "\tUser ${3:user}",
      "\tPort 22",
      "\tIdentityFile %d/.ssh/${4:ssh.pem}"
    ]
  }
}
