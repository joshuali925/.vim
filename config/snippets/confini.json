{
  "AWS profile from creds in ~/.aws/credentials": {
    "prefix": "template-aws-access-key",
    "body": [
      "[${1:my_profile}]",
      "aws_access_key_id = ${2:AWS_ACCESS_KEY_ID}",
      "aws_secret_access_key = ${3:AWS_SECRET_ACCESS_KEY}",
      "aws_region = us-west-2"
    ]
  },
  "AWS profile from assume role in ~/.aws/credentials": {
    "prefix": "template-aws-assume-role",
    "body": [
      "[${1:my_profile}]",
      "role_arn = arn:aws:iam::${2:000000000000}:role/${3:Admin}",
      "source_profile = ${4:default}",
      "# or if credentials are not to be assumed by a profile, comment out source_profile and use credential_source",
      "# credential_source = ${5|Ec2InstanceMetadata,EcsContainer,Environment|}",
      "region = us-west-2"
    ]
  }
}
